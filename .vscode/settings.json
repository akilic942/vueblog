{
	"editor.tokenColorCustomizations": {
		"textMateRules": [
		  {
			"scope": "entity.name.tag.template",
			"settings": {
			  "foreground": "#b4e3ff",
			  "fontStyle": "bold"
			}
		  },
		  {
			"scope": "entity.name.tag.script",
			"settings": {
			  "foreground": "#e07ee4",
			  "fontStyle": "bold"
			}
		  },
		  {
			"scope": "entity.name.tag.style",
			"settings": {
